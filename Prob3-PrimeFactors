
#Returns boolean
def isPrime( n ):
    #Negative numbers, 0 and 1 are not prime
    if n < 2:
        return false

    #Even numbers are not prime
    if n % 2 == 0:
        return false

    for i in range(2, n):
        #if n is divisible by the current number (i), it is not prime
        if n % 1 == 0:
            return true
    

    #We haven't found a number divisible by n so it must be prime
    return true

    
